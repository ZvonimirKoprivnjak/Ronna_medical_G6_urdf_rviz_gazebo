<?xml version="1.0"?>
<robot name="mobilni_robot">

    <material name="blue">
        <color rgba="0 0 0.5 1"/>
    </material>

    <material name="green">
        <color rgba="0 0.5 0 1"/>
    </material>

    <material name="orange">
        <color rgba="1 0.45 0.02345 1"/>
    </material>

    <material name="grey">
        <color rgba="0.5 0.5 0.5 1"/>
    </material>

    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="red">
        <color rgba="1 0 0 1"/>
